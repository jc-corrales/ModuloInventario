							<ObjectAttribute title="Número de elementos" text="Item Attribute" active="false" visible="true"/>
						</attributes>
						<firstStatus/>
						<secondStatus/>
					</ObjectListItem>
					<ObjectListItem intro="" title="Órdenes de compra sin revisar" number="" numberUnit="" numberState="None" type="Active" selected="false"
						showMarkers="false">
