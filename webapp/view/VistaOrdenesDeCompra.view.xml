<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.maestroDetalle.controller.VistaOrdenesDeCompra" xmlns="sap.m">
	<Page title="Categoría de Órdenes de compra" showHeader="true" showFooter="true" showNavButton="true">
