				<headerToolbar>
					<Toolbar enabled="true" visible="true" width="" design="Solid">
						<content>
							<SearchField placeholder="Search" showSearchButton="true" visible="true" width="100%"/>
						</content>
					</Toolbar>
				</headerToolbar>
				<items>
					<ObjectListItem intro="" title="Últimas órdenes de compra" number="" numberUnit="" numberState="None" type="Active" selected="false"
