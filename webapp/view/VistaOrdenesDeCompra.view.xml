							<ObjectAttribute title="Número de elementos" text="Item Attribute" active="false" visible="true"/>
						</attributes>
						<firstStatus/>
						<secondStatus/>
					</ObjectListItem>
				</items>
			</List>
		</content>
		<footer>
			<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
